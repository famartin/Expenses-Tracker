<html>
<head>
<style>
	table, th, td {
		border: 1px solid black;
	}
</style>
</head>
<body>
<%- include('nav'); %>
<table style="width:100%">
<tr>
	<th>Date</th>
	<th>Amount</th>
</tr>
<% deposits.forEach(function(deposit){ %>
	<tr><td><%= deposit.date.toDateString() %></td><td><a href="/cancel-deposit/<%= deposit._id %>">$<%= deposit.amount.toFixed(2) %></a></td></tr>
<% }); %>
</table>
</body>
</html>
